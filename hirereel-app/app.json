{
  "expo": {
    "name": "hirereel-app",
    "slug": "hirereel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hirereel.hirereel"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-video",
          {
            "supportsBackgroundPlayback": true,
            "supportsPictureInPicture": true
          },
          "expo-camera",
          {
            "cameraPermission": "Allow HireReel to access your camera",
            "microphonePermission": "Allow HireReel to access your microphone",
            "recordAudioAndroid": true
          }
        ]
      ]
    },
    "extra": {
      "eas": {
        "projectId": "c3441ed6-e7ca-4adc-bbb7-c2d72b800226"
      }
    },
    "owner": "hirereel"
  }
}
